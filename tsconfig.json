{
    "compilerOptions": {
        "lib": ["es2020"],
      "module": "esnext",
      "moduleResolution": "node",
      "target": "es2020",
      "outDir": "./dist",
      "rootDir": "./src",
      "emitDeclarationOnly": false,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "allowSyntheticDefaultImports": true,
      "skipLibCheck": true,
      "skipDefaultLibCheck": true,
      "resolveJsonModule": true,
      "sourceMap": true
    },
    "include": ["./src/**/*.ts"],
    "exclude": ["node_modules"]
    }